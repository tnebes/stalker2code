{
    "comments": {
        "lineComment": "//",
        "blockComment": [
            "/*",
            "*/"
        ]
    },
    "brackets": [
        [
            "{",
            "}"
        ],
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ]
    ],
    "autoClosingPairs": [
        {
            "open": "{",
            "close": "}"
        },
        {
            "open": "[",
            "close": "]"
        },
        {
            "open": "(",
            "close": ")"
        },
        {
            "open": "\"",
            "close": "\"",
            "notIn": [
                "string"
            ]
        },
        {
            "open": "'",
            "close": "'",
            "notIn": [
                "string"
            ]
        }
    ],
    "surroundingPairs": [
        [
            "{",
            "}"
        ],
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ],
        [
            "\"",
            "\""
        ],
        [
            "'",
            "'"
        ]
    ],
    "indentation": {
        "increaseIndentPattern": "^.*struct\\.begin.*$",
        "decreaseIndentPattern": "^\\s*struct\\.end.*$"
    },
    "folding": {
        "markers": {
            "start": "^.*struct\\.begin.*$",
            "end": "^\\s*struct\\.end.*$"
        }
    }
}